DECLARE @Variable1 NVARCHAR(50) = 'variable1'
DECLARE @Variable2 NVARCHAR(50) = 'variable2'
DECLARE @Variable3 NVARCHAR(50) = 'variable3'

-- Agar aapke paas sirf 1 variable hai toh uske hisaab se query likhe
Update EAS_JOBS 
SET Status_ind = '300' 
WHERE job_ID IN (
    CASE WHEN @Variable1 IS NOT NULL THEN @Variable1 END,
    CASE WHEN @Variable2 IS NOT NULL THEN @Variable2 END,
    CASE WHEN @Variable3 IS NOT NULL THEN @Variable3 END
);

