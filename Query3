' Generate timestamp components
sDate = DatePart("yyyy", Now) & ZeroFill(DatePart("m", Now), 2) & ZeroFill(DatePart("d", Now), 2)
HoursMinSecond = ZeroFill(DatePart("h", Now), 2) & ZeroFill(DatePart("n", Now), 2) & ZeroFill(DatePart("s", Now), 2)

' Construct the CSV file name
DATAFilePath = "\\" & Execserver & "\Server\EXPORTS\VNDLYEXPORTS\vt_9f3808fd_1269_423c_a745_879be1f28416_inv_payments_" & sDate & "_" & HoursMinSecond & ".csv"
