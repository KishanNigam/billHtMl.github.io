@echo off
REM Set the network folder path
set "folderPath=\\nicoeas12\server\CHASH.dat"

REM Check if the folder exists at the specified network path
if exist "%folderPath%" (
    echo Renaming folder '%folderPath%' to 'import'...
    pushd \\nicoeas12\server
    ren "CHASH.dat" "import"
    popd
    echo Folder renamed successfully.
) else (
    echo Folder '%folderPath%' does not exist.
)
pause
