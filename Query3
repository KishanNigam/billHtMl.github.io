@echo off
setlocal

set logDir1=\\nig\eas\log\
set logDir2=\\nig\eas\log2\

REM Get the latest file from logDir1
for /f "delims=" %%a in ('dir "%logDir1%" /b /a-d /o-d') do (
    set latestFile=%%a
    goto :copyFile
)

:copyFile
if defined latestFile (
    echo Copying "%logDir1%%latestFile%" to "%logDir2%"
    copy "%logDir1%%latestFile%" "%logDir2%"
) else (
    echo No files found in %logDir1%.
)

endlocal


