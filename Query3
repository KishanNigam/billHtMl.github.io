IF LEN(TRANNUM) < 6 THEN
    NEXTLINE = arr(2) 
    IF LEFT(NEXTLINE, 3) = "88," THEN
        TRANEXT = MID(NEXTLINE, 4) 
        TRANEND = INSTR(TRANEXT, "/") 
        IF TRANEND > 0 THEN
            TRANEXT = LEFT(TRANEXT, TRANEND - 1) 
        END IF
        TRANNUM = TRANNUM & TRANEXT 
    END IF
END IF



IF LEN(TRANNUM) < 6 THEN
    NEXTLINE = arr(2) 
    IF LEFT(NEXTLINE, 3) = "88," THEN
        TRANEXT = MID(NEXTLINE, 4, INSTR(NEXTLINE, "/") - 4) 
        TRANNUM = TRANNUM & TRANEXT 
    END IF
END IF
