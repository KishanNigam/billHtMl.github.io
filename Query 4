REM Assign JOB_ID variables from session_lock.txt, handling missing lines
SET idx=1
FOR /F "usebackq tokens=*" %%L IN ("%sourcedir%%sourcefile%") DO (
    SET "line=%%L"
    SET "line=!line: =!"  REM Remove all spaces from the line
    SET "JOB_ID_!idx!=!line!"
    SET /A idx+=1
    IF !idx! GTR 3 GOTO Done  REM Stop if we have set JOB_ID_1, JOB_ID_2, and JOB_ID_3
)

:Done
REM Display assigned values (for testing)
echo JOB_ID_1=%JOB_ID_1%
echo JOB_ID_2=%JOB_ID_2%
echo JOB_ID_3=%JOB_ID_3%
