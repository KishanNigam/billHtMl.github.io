REM Logic to Check Last Character of Filename
set lastchar=%sourcefile:~26,1%
echo lastchar = [%lastchar%]    >>%logfilename%

if "%lastchar%"=="E" (
    set archivefilename=%sourcefile:~0,27%
    echo EEE archivefilename = [%archivefilename%]   >>%logfilename%
) else (
    set archivefilename=%sourcefile:~0,29%
    echo archivefilename = [%archivefilename%]   >>%logfilename%
)
